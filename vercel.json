{
	"version": 2,
	"builds": [
		{
			"src": "/back-end/api/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/back-end/api/index.js",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE",
				"PATCH",
				"OPTIONS"
			],
			"headers": {
				"Access-Control-Allow-Origin": "*"
			}
		}
	],
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api"
		}
	]
}